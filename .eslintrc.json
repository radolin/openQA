{
  "env": {
    "browser": true,
    "jquery": true,
    "es2021": true
  },
  "extends": [
    "semistandard"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": ".*"
      }
    ]
  }
}
